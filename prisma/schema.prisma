generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Team {
  id          Int      @id @default(autoincrement())
  userName    String
  userRole    String
  userImage   String
  projectName String
  teamImages  String[]
  status      String
}

model Event {
  id        String   @id @default(uuid())
  title     String
  startDate DateTime
  endDate   DateTime
  level     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
